{
  "name": "epr-backend",
  "version": "0.0.0",
  "description": "epr-backend",
  "main": "src/index.js",
  "type": "module",
  "imports": {
    "#adapters/*": "./src/adapters/*",
    "#application/*": "./src/application/*",
    "#common/*": "./src/common/*",
    "#data/*": "./src/data/*",
    "#domain/*": "./src/domain/*",
    "#feature-flags/*": "./src/feature-flags/*",
    "#formsubmission/*": "./src/forms-submission-data/*",
    "#glass-migration/*": "./src/glass-migration/*",
    "#plugins/*": "./src/plugins/*",
    "#repositories/*": "./src/repositories/*",
    "#root/*": "./src/*",
    "#routes/*": "./src/routes/*",
    "#server/*": "./src/server/*",
    "#test/*": "./src/test/*",
    "#vite/*": "./.vite/*",
    "#workers/*": "./src/workers/*"
  },
  "engines": {
    "node": ">=24.11.0",
    "npm": ">=11.6.1"
  },
  "scripts": {
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "postinstall": "npm run setup:husky",
    "lint": "eslint --cache --cache-strategy content \"**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "benchmark:file": "node benchmarks/parse-file.js",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest --coverage",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "schema:generate": "node scripts/generate-json-schemas/index.js",
    "schema:sync-local": "npm run schema:generate && cp .schemas/organisation.update.json ../epr-re-ex-admin-frontend/src/server/common/schemas/organisation.json && npm run schema:compile --prefix ../epr-re-ex-admin-frontend"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.972.0",
    "@aws-sdk/credential-providers": "3.972.0",
    "@defra/cdp-auditing": "0.6.0",
    "@defra/hapi-secure-context": "0.4.0",
    "@defra/hapi-tracing": "1.30.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@fast-csv/format": "5.0.5",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.4",
    "@hapi/inert": "7.1.0",
    "@hapi/jwt": "3.2.3",
    "@hapi/vision": "7.0.3",
    "aws-embedded-metrics": "4.2.1",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "deepmerge": "4.3.1",
    "exceljs": "4.4.0",
    "global-agent": "3.0.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "hapi-swagger": "17.3.2",
    "http-status-codes": "2.3.0",
    "immer": "11.1.3",
    "joi": "17.13.3",
    "joi-to-json": "5.0.1",
    "lodash.chunk": "4.2.0",
    "lodash.keyby": "4.6.0",
    "lodash.mergewith": "4.6.2",
    "lodash.partition": "4.6.0",
    "mongo-locks": "3.1.2",
    "mongodb": "7.0.0",
    "notifications-node-client": "8.2.1",
    "obfuscate-mail": "1.5.1",
    "pino": "10.2.1",
    "pino-pretty": "13.1.3",
    "piscina": "5.1.4",
    "undici": "7.18.2"
  },
  "overrides": {
    "brace-expansion": "2.0.2",
    "glob": "11.1.0",
    "validator": "13.15.23"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.17",
    "eslint": "9.39.2",
    "eslint-plugin-n": "17.23.2",
    "husky": "9.1.7",
    "knip": "5.82.1",
    "lint-staged": "16.2.7",
    "msw": "2.12.7",
    "neostandard": "0.12.2",
    "nodemon": "3.1.11",
    "prettier": "3.8.0",
    "testcontainers": "11.11.0",
    "tinybench": "6.0.0",
    "vitest": "4.0.17",
    "vitest-fetch-mock": "0.4.5",
    "vitest-mongodb": "1.0.3"
  }
}
